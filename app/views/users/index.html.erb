<h1>Users</h1>

<%= link_to "Logout", logout_path, method: :delete %>

<ul>
  <% @users.each do |user| %>
    <li>
      <%= user.name %> -
      <%= link_to "Show", user %> |
      <%= link_to "Edit", edit_user_path(user) %> |
      <%= link_to "Delete", user, method: :delete, data: { confirm: "Are you sure?" } %>
    </li>
  <% end %>
</ul>
